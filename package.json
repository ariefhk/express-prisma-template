{
  "name": "express-prisma-template",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": ".src/main.js",
  "scripts": {
    "postinstall": "npx prisma generate",
    "prestart": "npm run build",
    "start": "node ./src/main.js",
    "dev": "nodemon ./src/main.js",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push --force-reset && npx prisma db push",
    "db:push:seed": "npm run db:push && npm run db:seed",
    "db:seed": "node ./src/db/seeder/seed.js",
    "test": "jest -i --no-cache --coverage --colors --verbose --detectOpenHandles --forceExit"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "jose": "^5.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  }
}
